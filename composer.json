{
  "name": "solosgigifaye/seat-info-toastui",
  "description": "A drop-in replacement for seat-info that swaps Ace for TOAST UI Editor.",
  "type": "seat-plugin",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Community Maintainers",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "replace": {
    "recursivetree/seat-info": "self.version"
  },
  "autoload": {
    "psr-4": {
      "RecursiveTree\\Seat\\InfoPlugin\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RecursiveTree\\Seat\\InfoPlugin\\InfoServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
